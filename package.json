{
  "name": "studioconnect-ai",
  "version": "1.0.0",
  "description": "AI-powered client communication and lead qualification platform for creative agencies",
  "main": "index.js",
  "scripts": {
    "test": "echo \"No tests specified for StudioConnect AI yet\" && exit 0",
    "dev": "npm-run-all --parallel dev:api dev:dashboard tailwind:watch",
    "dev:api": "nodemon src/server.ts",
    "dev:dashboard": "npm --prefix dashboard run dev -- -p 3100",
    "build": "npx prisma generate && tsc",
    "postbuild": "mkdir -p dist/views && cp -R src/views/* dist/views/",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "tailwind:build": "tailwindcss -i ./assets/tailwind.css -o ./public/css/ui.css --minify",
    "tailwind:watch": "tailwindcss -i ./assets/tailwind.css -o ./public/css/ui.css --watch --minify",
    "build:dashboard": "npm --prefix dashboard run build",
    "start:dashboard": "npm --prefix dashboard run start -- -p 3100",
    "postinstall": "npm --prefix dashboard install --legacy-peer-deps"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@prisma/client": "^6.9.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.29",
    "@types/nodemailer": "^6.4.17",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.5",
    "prisma": "^6.9.0",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@openai/agents": "0.0.5",
    "@types/twilio": "^3.19.2",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "4",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts",
    "next": "^14.1.0",
    "next-auth": "^4.24.5",
    "next-connect": "1.0.0",
    "nodemailer": "^6.9.12",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "openai": "^5.0.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.7.1",
    "twilio": "^5.7.0",
    "ws": "^8.18.2",
    "zod": "^3.22.4"
  }
}
